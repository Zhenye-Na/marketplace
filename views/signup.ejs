<% include partials/header %>

<h1>Sign up</h1>

<form action="/signup" method="POST">
    <div class="form-group">
        <input type="text" name="username" placeholder="email">
    </div>
    <div class="form-group">
        <input type="password" name="password" placeholder="password">
    </div>
    <div class="form-group">
        <button>Sign up</button>
    </div>
</form>